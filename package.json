{
  "name": "ilia-nodejs-challenge",
  "version": "1.0.0",
  "main": "build/server.js",
  "author": "The-Kevin <kevin.almeida1111.ka@gmail.com>",
  "license": "MIT",
  "scripts": {
    "local": "export NODE_ENV=local && nodemon src/server.ts",
