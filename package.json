  "dependencies": {
    "@mobixtec/visse": "^1.1.8",
    "chalk": "^5.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^2.28.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.13.2",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.13",
    "@types/express-validator": "^3.0.0",
    "@types/faker": "^5.1.6",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "5.7.32",
    "@types/node": "^14.14.25",
    "@types/passport": "^1.0.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^2.0.7",
    "prettier": "2.4.0",
    "sucrase": "^3.18.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^10.6.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.+(ts|js)"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "lcov"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  }
}
